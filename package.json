{
  "name": "kohlrabi",
  "version": "0.1.0",
  "description": "Beautiful, modern API documentation from OpenAPI specs. Zero config, just add your swagger.json.",
  "type": "module",
  "bin": {
    "kohlrabi": "bin/cli.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "dev": "node bin/cli.js serve",
    "build": "node bin/cli.js build",
    "prepublishOnly": "echo 'Ready to publish!'"
  },
  "keywords": [
    "openapi",
    "swagger",
    "api",
    "documentation",
    "docs",
    "rest",
    "api-docs"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adambeck7/kohlrabi.git"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/adambeck7/kohlrabi/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "vite": "^5.4.0",
    "@apidevtools/swagger-parser": "^10.1.0"
  }
}
